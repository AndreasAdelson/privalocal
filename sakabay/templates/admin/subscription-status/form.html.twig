{% extends 'base.html.twig' %}

{% block title %}
  {% if susbcriptionStatusId is same as("null") %}
    {{ 'subscription_status_title_create_page'|trans }}
  {% else %}
    {{ 'subscription_status_title_edit_page'|trans }}
  {% endif %}
{% endblock %}

{% block body %}
  {% if susbcriptionStatusId is same as("null") %}
    <h1 class="fontUbuntuItalic orange-skb text-center p-1">{{ 'subscription_status_title_create_page'|trans }}</h1>
  {% else %}
    <h1 class="fontUbuntuItalic orange-skb text-center p-1">{{ 'subscription_status_title_edit_page'|trans }}</h1>
  {% endif %}
<form-subscription-status :subscription-status-id="{{ susbcriptionStatusId }}" token="{{ csrf_token('subscriptionStatus') }}"></form-subscription-status>
{% endblock %}
